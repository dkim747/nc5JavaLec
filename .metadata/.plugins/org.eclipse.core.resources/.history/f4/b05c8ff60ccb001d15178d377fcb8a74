package chap999_homework_2023_03_16;

public class _homeworkBasic03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = new int[10];
		int index = 0;
		
		for(int i=1; index<10; i++) {
			if(i%2==1) {
				arr[index++]=i;
				//System.out.println(index[i]);
			}
		}
	}

}

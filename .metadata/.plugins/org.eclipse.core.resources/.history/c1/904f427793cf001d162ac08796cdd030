package chap19_multithread;

public class _02_anonymousThread {

	public static void main(String[] args) {
		
		//1. 스레드의 정의
		Thread t= new Thread() {
			@Override
			public void run() {
				for(int i = 1; i <=5; i++) {
					System.out.println(i);
					
					Thread.sleep(500);
				}
			}
		}
	}

}

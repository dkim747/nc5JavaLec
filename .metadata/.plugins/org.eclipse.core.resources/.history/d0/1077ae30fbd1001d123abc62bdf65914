package chap999_homework_2023_04_02_class;

public class No1 {
	
	public String stnName;
	public int stnNum;
	public String major;
	
	public No1(){
		
	}
	
	public void insertStnInfo(String stnName, int stnNum, String major) {
		this.stnName = stnName;
		this.stnNum = stnNum;
		this.major = major;
	}
	
	public void printStnInfo() {
		System.out.println(this.stnName + this.stnNum + this.major);
	}
}

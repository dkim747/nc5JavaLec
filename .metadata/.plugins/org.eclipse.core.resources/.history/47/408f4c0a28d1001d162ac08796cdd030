package practice;

import java.util.Scanner;

public class qeqwe {
    public static void main(String[] args) {
        
    	Scanner sc = new Scanner(System.in);
    	int input = sc.nextInt();
    	
    	for(int i = 2; i <= input; i++) {
    		for(int j = 1; j <=9; j++) {
    			System.out.println(i + "*" + j + "=" + i * j);
    		}
    	}
    	
    	
    }
}


package chap999_homework_2023_03_16;

public class _homeworkBasic01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
				
		int[] arr = new int[10];
		int num = 0;
		for(int i = 0; i < arr.length; i++) {
			if (num % 3 == 0) {
				arr[i] = num++;
				
			}
		}System.out.println(arr[0]);
		
		
		
		
		
		
		
		
	
	
			
			
		
	}

}

		//2.
		Scanner sc = new Scanner(System.in);
		
		ArrayList<Integer> list = new ArrayList<Integer>();
		while(true) {
			System.out.println("정수를 입력하세요");
			String input = sc.nextLine();
			
			if(input.equals("q")){
				break;
			}
			
			int num = Integer.parseInt(input);
			list.add(num);	
		}
		System.out.println(num);
	}

}

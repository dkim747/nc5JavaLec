package chap999_homework_2023_03_16;

import java.util.Random;

public class _homeworkBasic03 {

	public static void main(String[] args) {
		
		int[] arr = new int[10];
		Random rand = new Random(10);
		for(int i = 0; i < arr.length; i++) {
			arr[i] = rand.nextInt();
			if(i%2==1) {
				System.out.println(arr[i]);
			}
		}
		
			
		
	}

}

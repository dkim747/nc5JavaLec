package chap999_homework_2023_03_16;

public class _homeworkBasic01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
				
		int[] arr = new int[10];
		int num = 1;
		int count = 0;
		
		while(true){
			if(num % 3 == 0) {
				arr[count++]=num;
				num++;
			}
		}
			
	}	

}

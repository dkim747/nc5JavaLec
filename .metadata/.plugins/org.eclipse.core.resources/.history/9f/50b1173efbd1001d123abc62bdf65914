package chap999_homework_2023_04_02;

import java.util.Scanner;

import chap999_homework_2023_04_02_class.No1;

public class _02_class {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		No1 stn = new No1();
		
		System.out.println("학생 정보를 입력하세요. 이름 학번 전공");
		stn.stnName = sc.nextLine();
		stn.stnNum = sc.nextInt();
		sc.nextLine();
		stn.major = sc.nextLine();
		
		stn.printStnInfo();
	}

}
